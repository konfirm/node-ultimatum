{
  "name": "ultimatum",
  "version": "1.0.4",
  "description": "Smarter Timings. Run at intervals, set timeouts, schedule at specific times, postpone intervals/timeouts",
  "main": "lib/ultimatum.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "posttest": "./node_modules/.bin/nsp check",
    "test": "./node_modules/.bin/lab -f -t 100 -m 3000",
    "test-coverage": "./node_modules/.bin/lab -f -r html -m 3000 -o report.html"
  },
  "author": "Rogier Spieker",
  "license": "GPLv2",
  "bugs": {
    "url": "https://github.com/konfirm/node-ultimatum/issues"
  },
  "homepage": "https://github.com/konfirm/node-ultimatum",
  "dependencies": {},
  "devDependencies": {
    "code": "^3.0.1",
    "coveralls": "^2.11.9",
    "lab": "^10.8.2",
    "nsp": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/konfirm/node-ultimatum.git"
  },
  "keywords": [
    "timer",
    "timing",
    "interval",
    "schedule"
  ],
  "engines": {
    "node": ">=4"
  }
}
